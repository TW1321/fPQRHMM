\name{check_lossTE}
\alias{check_lossTE}
\title{Computes check loss for quantile regression model}
\usage{
check_lossTE(beta_pred, X_test, Y_test, q)
}
\arguments{
\item{beta_pred}{coefficients for mode:}

\item{X_test}{test set for predictor}

\item{Y_test}{test set for response}

\item{q}{quantile predicting}
}
\value{
check loss error
}
\description{
Computes check loss for quantile regression model
}
\examples{
X_0=mvrnorm(100, mu=rep(0,100), Sigma=diag(100))
 beta=rep(1,100)
 Y_0= X_0\%*\%beta+rnorm(100)
 fPQR_mod= fPQR_fit(X_0, Y_0, 0.5)
 est_coef=fPQR_mod[[1]]
 check_losserror=check_lossTE(est_coef, X_0, Y_0,0.5)
}
